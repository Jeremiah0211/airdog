<?xml version="1.0" encoding="utf-8"?>
<mx:Canvas xmlns:mx="http://www.adobe.com/2006/mxml"
	 xmlns:controller="no.airdog.controller.*" width="100%">
	<controller:FilopplastController id="controller" creationComplete="controller.start(pBar,uploadtekst);" />
	<mx:Panel width="50%"  verticalAlign="middle" horizontalAlign="center" id="opplastingPanel" title="Last opp fil" paddingTop="5" paddingRight="5" paddingLeft="5" paddingBottom="5">
