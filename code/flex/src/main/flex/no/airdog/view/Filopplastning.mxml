		<mx:ProgressBar id="pBar" mode="manual" label="" width="90%"  labelPlacement="center" visible="false" trackHeight="10"/>
		<mx:TextArea width="90%" height="200" id="uploadtekst" visible="false"/>
	</mx:Panel>
</mx:Canvas>
